{
  "name": "dyloader",
  "version": "0.1.0",
  "description": "Dyloader(dynamicLoader) is a JavaScript library for loading module(without router)",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "lint": "eslint --cache --ext .ts,.tsx ./",
    "test": "NODE_ENV=unittest jest",
    "coverage": "codecov"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "dynamic",
    "module",
    "loader"
  ],
  "bugs": {
    "url": "https://github.com/daysai/dynamicLoader/issues"
  },
  "homepage": "https://github.com/daysai/dynamicLoader",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysai/dynamicLoader.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@ice/spec": "^0.1.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "codecov": "^3.4.0",
    "eslint": "^5.16.0",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "jest-fetch-mock": "^2.1.2",
    "stylelint": "^10.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "ts-jest",
    "automock": false,
    "setupFiles": [
      "./setupJest.ts"
    ]
  }
}
